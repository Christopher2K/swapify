{
  "extends": "astro/tsconfigs/strict",
  "include": ["src", "styled-system"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "#root/*": ["./src/*"],
      "#style/*": ["./styled-system/*"],
      "~/*": ["./src/*"],
      "styled-system/*": ["./styled-system/*"]
    },
    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["vite-plugin-svgr/client", "musickit-js"]
  }
}
